{"ast":null,"code":"import MainCarouselVue from \"@/components/MainCarousel.vue\";\nimport ProductItem from \"@/components/ProductItem.vue\";\nimport ProductsAPI from \"@/services/products\";\nexport default {\n  components: {\n    MainCarouselVue,\n    ProductItem\n  },\n  data: () => ({\n    listProducts: [],\n    search: \"\"\n  }),\n  methods: {\n    getProducts() {\n      ProductsAPI.product().then(result => {\n        try {\n          const {\n            data\n          } = result;\n          this.listProducts = data;\n        } catch (e) {\n          console.log(e);\n        }\n      });\n    }\n\n  },\n  computed: {\n    filteredProducts: () => {\n      return this.listProducts.filter(product => {\n        return product.name.toLowerCase().indexOf(this.search.toLowerCase()) != -1;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAwCA,OAAOA,eAAP,MAA4B,+BAA5B;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,WAAP,MAAwB,qBAAxB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,mBADU;AAEVC;AAFU,GADC;AAKbG,MAAI,EAAE,OAAO;AACXC,gBAAY,EAAE,EADH;AAEXC,UAAM,EAAE;AAFG,GAAP,CALO;AASbC,SAAO,EAAE;AACPC,eAAW,GAAG;AACZN,iBAAW,CAACO,OAAZ,GAAsBC,IAAtB,CAA4BC,MAAD,IAAY;AACrC,YAAI;AACF,gBAAM;AAAEP;AAAF,cAAWO,MAAjB;AACA,eAAKN,YAAL,GAAoBD,IAApB;AACF,SAHA,CAGE,OAAOQ,CAAP,EAAU;AACVC,iBAAO,CAACC,GAAR,CAAYF,CAAZ;AACF;AACD,OAPD;AAQD;;AAVM,GATI;AAqBbG,UAAQ,EAAE;AACRC,oBAAgB,EAAE,MAAM;AACtB,aAAO,KAAKX,YAAL,CAAkBY,MAAlB,CAA0BR,OAAD,IAAa;AAC3C,eACEA,OAAO,CAACS,IAAR,CAAaC,WAAb,GAA2BC,OAA3B,CAAmC,KAAKd,MAAL,CAAYa,WAAZ,EAAnC,KAAiE,CAAC,CADpE;AAGD,OAJM,CAAP;AAKD;AAPO;AArBG,CAAf","names":["MainCarouselVue","ProductItem","ProductsAPI","components","data","listProducts","search","methods","getProducts","product","then","result","e","console","log","computed","filteredProducts","filter","name","toLowerCase","indexOf"],"sourceRoot":"","sources":["C:\\Users\\dragu\\Dev\\Desafios\\ow-interactive-desafio\\src\\views\\ProductsPage.vue"],"sourcesContent":["<template>\r\n  <MainCarouselVue />\r\n\r\n  <div>\r\n    <div class=\"container search--container\">\r\n      <div class=\"row align-items-center\">\r\n        <div class=\"col-4\">\r\n          <p class=\"search--title\">Encontre seu produto</p>\r\n        </div>\r\n        <div class=\"col-8\">\r\n          <div class=\"search__input--container\">\r\n            <input v-model=\"search\" type=\"text\" placeholder=\"Pesquisar...\" />\r\n            <button>\r\n              <img src=\"../assets/icons/magnifying-glass.svg\" alt=\"\" />\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"container mt-4\">\r\n      <div class=\"row\">\r\n        <div\r\n          v-for=\"(product, i) in filteredProducts\"\r\n          :key=\"{{i}}\"\r\n          class=\"col-lg-4\"\r\n        >\r\n          <ProductItem></ProductItem>\r\n        </div>\r\n        <div class=\"col-lg-4\">\r\n          <ProductItem></ProductItem>\r\n        </div>\r\n        <div class=\"col-lg-4\">\r\n          <ProductItem></ProductItem>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MainCarouselVue from \"@/components/MainCarousel.vue\";\r\nimport ProductItem from \"@/components/ProductItem.vue\";\r\nimport ProductsAPI from \"@/services/products\";\r\n\r\nexport default {\r\n  components: {\r\n    MainCarouselVue,\r\n    ProductItem,\r\n  },\r\n  data: () => ({\r\n    listProducts: [],\r\n    search: \"\",\r\n  }),\r\n  methods: {\r\n    getProducts() {\r\n      ProductsAPI.product().then((result) => {\r\n        try {\r\n          const { data } = result;\r\n          this.listProducts = data;\r\n        } catch (e) {\r\n          console.log(e);\r\n        }\r\n      });\r\n    },\r\n  },\r\n  computed: {\r\n    filteredProducts: () => {\r\n      return this.listProducts.filter((product) => {\r\n        return (\r\n          product.name.toLowerCase().indexOf(this.search.toLowerCase()) != -1\r\n        );\r\n      });\r\n    },\r\n  },\r\n};\r\n<